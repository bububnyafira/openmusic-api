{
  "name": "openmusic-api",
  "version": "2.0.0",
  "description": "Open-source music player API with user authentication and playlist features",
  "main": "src/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node src/server.js",
    "start:dev": "nodemon src/server.js",
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "lint": "eslint ./src"
  },
  "keywords": [
    "music",
    "api",
    "open-source",
    "authentication",
    "playlists"
  ],
  "author": "OpenMusic Team",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/s3-request-presigner": "^3.637.0",
    "@hapi/hapi": "^21.3.2",
    "@hapi/jwt": "^3.2.0",
    "amqplib": "^0.10.4",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "nanoid": "^3.3.6",
    "node-pg-migrate": "^7.5.2",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.0.1",
    "pg": "^8.12.0",
    "redis": "^4.7.0"
  }
